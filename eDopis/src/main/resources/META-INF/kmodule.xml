<kmodule xmlns="http://jboss.org/kie/6.0.0/kmodule" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <kbase name="mykbase" default="true" eventProcessingMode="cloud" equalsBehavior="identity">
    <ksession name="myksession" type="stateful" default="false" clockType="realtime">
      <workItemHandlers>
        <workItemHandler type="org.jugvale.jbpm.workitemhandler.HelloWorkItemHandler" name="HelloWorkItemHandler"/>
      </workItemHandlers>
    </ksession>
    <ksession name="slanjesession" type="stateful" default="true" clockType="realtime">
      <workItemHandlers>
        <workItemHandler type="org.jugvale.jbpm.workitemhandler.SlanjeWorkItemHandler" name="SlanjeWorkItemHandler"/>
        <workItemHandler type="org.jugvale.jbpm.workitemhandler.OdgovorWorkItemHandler" name="OdgovorWorkItemHandler"/>
      </workItemHandlers>
    </ksession>
  </kbase>
</kmodule>